#1 рбота со списком, по очереди число(+\-), слово выявляем тип с помощью type
my_list = [12, Нет, -77, 'Истина', Истина, 9.5]
def my_type(el):
 для el в диапазоне(len(my_list)):
        print(type(my_list[el]))
 возвращать
my_type(my_list)

#2 создаем список через запрос пользователя, добавляем append(кидает в конец)
el_count = int(input("Введите количество элементов списка "))
my_list = []
i = 0
el = 0
в то время как я < el_count:
    my_list.append(input("Введите следующее значение списка "))
    i += 1

для elem в диапазоне(int(len(my_list)/2)):
        my_list[el], my_list[el + 1] = my_list [el + 1], my_list[el]
        el += 2
печать(my_list)

#3 задаем список сезонов через лист, запрос у пользователя цифры от 1 до 12
seasons_list = ['зима', 'весна', 'лето', 'осень']
seasons_dict = {1 : 'зима', 2 : 'весна', 3 : 'лето', 4 : 'осень'}
month = int(input("Введите месяц по номеру "))
если месяц ==12 или месяц == 1 или месяц == 2:
 печать(seasons_dict.get(1))
 печать(seasons_list[0])
элиф месяц == 3 или месяц == 4 или месяц ==5:
 печать(seasons_dict.get(2))
 печать(seasons_list[1])
элиф месяц == 6 или месяц == 7 или месяц == 8:
 печать(seasons_dict.get(3))
 печать(seasons_list[2])

элиф месяц == 9 или месяц == 10 или месяц == 11:
 печать(seasons_dict.get(4))
 печать(seasons_list[3])
еще:
        print("Такого месяца не существует")
        
        
        
#4 перенос строки

my_str = input("Введите слова в строку")
my_word = []
число = 1
для el в диапазоне(my_str.count(' ') + 1):
    my_word = my_str.split()
 если len(str(my_word)) <= 10:
        print(f" {num} {my_word [el]}")
 число += 1
 еще:
        print(f" {num} {my_word [el] [0:10]}") #[0:10] в зависимости от цифры перенос изменится
 число += 1
        
#5 рейтинг чисел от - до +

my_list = [7, 5, 3, 3, 2]
print(f"Рейтинг - {my_list}") #литерал f для лучшей читаемости кода (ф строка перед ковычками)
digit = int(input("Введите число (для выхода введите 369) ")) #digit возвращает true если в стркое все цифры
в то время как цифра != 36967:
 для el в диапазоне(len(my_list)):
 если my_list[el] == цифра:
 my_list.insert(el + 1, цифра)
 ломать
 elif my_list[0] < цифра:
 my_list.insert(0, цифра)
 elif my_list[-1] > цифра:
 my_list.append(цифра)
 elif my_list[el] > цифра и my_list[el + 1] < цифра:
 my_list.insert(el + 1, цифра)
    print(f"текущий список - {my_list}")
    digit = int(input("Введите число "))
